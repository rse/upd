{
    "name":        "upd",
    "homepage":    "http://github.com/rse/upd",
    "description": "Upgrade NPM Package Dependencies",
    "version":     "2.9.5",
    "license":     "MIT",
    "author": {
        "name":    "Dr. Ralf S. Engelschall",
        "email":   "rse@engelschall.com",
        "url":     "http://engelschall.com"
    },
    "type": "commonjs",
    "keywords": [
        "upgrade", "package", "dependency", "npm", "package.json"
    ],
    "repository": {
        "type": "git",
        "url":  "git+https://github.com/rse/upd.git"
    },
    "bugs": {
        "url":  "http://github.com/rse/upd/issues"
    },
    "bin": {
        "upd":  "upd.js"
    },
    "devDependencies": {
        "eslint":                 "8.57.0",
        "eslint-config-standard": "17.1.0",
        "eslint-plugin-promise":  "6.1.1",
        "eslint-plugin-import":   "2.29.1",
        "eslint-plugin-node":     "11.1.0"
    },
    "dependencies" : {
        "yargs":                  "17.7.2",
        "semver":                 "7.6.0",
        "awaity":                 "1.0.0",
        "pacote":                 "17.0.6",
        "progress":               "2.0.3",
        "pretty-bytes":           "5.6.0",
        "update-notifier":        "5.1.0",
        "fast-diff":              "1.3.0",
        "cli-table3":             "0.6.3",
        "ducky":                  "2.8.2",
        "escape-string-regexp":   "4.0.0",
        "chalk":                  "4.1.2",
        "strip-ansi":             "6.0.0",
        "micromatch":             "4.0.5",
        "json-asty":              "1.2.1"
    },
    "upd": [
        "!pretty-bytes",
        "!escape-string-regexp",
        "!strip-ansi",
        "!execa",
        "!chalk",
        "!update-notifier"
    ],
    "engines": {
        "node":                   ">=16.0.0"
    },
    "scripts": {
        "prepublishOnly":         "eslint --config eslint.yaml upd.js"
    }
}
